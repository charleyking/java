package club.charleyking.algorithm;

public class Sort {
	public static void bubbleSort(int[] arr) {
		int length = arr.length;
		int j, i;
		for (j = 0; j < length; j++) {
			for (i = 0; i < length - j; i ++) {
				if (arr[i] >arr[i+1]) {
					int temp = arr[i+1];
					arr[i+1] = arr[i];
					arr[i] = temp;
				}
			}
		}
		for (int a: arr){
			System.out.print(a);
		}
	}
	public static void swap() {
	}
}
